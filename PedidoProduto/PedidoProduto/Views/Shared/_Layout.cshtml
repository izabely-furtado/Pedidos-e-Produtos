<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- FAVICON -->
    <link rel="icon" type="image/png" sizes="192x192" href="~/assets/img/favicon/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/assets/img/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/assets/img/favicon/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/assets/img/favicon/favicon-16x16.png">
    <link rel="manifest" href="~/assets/img/favicon/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="~/assets/img/favicon/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">

    <title>@ViewData["Title"] - PedidoProduto</title>

    <!-- Styles -->
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/assets/css/owl.carousel.css" rel="stylesheet">
    <link href="~/assets/css/owl.theme.css" rel="stylesheet">
    <link href="~/assets/css/owl.transitions.css" rel="stylesheet">
    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Lato:300,300i,400,400i,700,700i,900,900i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Oswald:300,400,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed:300,400" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/assets/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/flaticon.css">

    <!-- Light Gallery -->
    <link href="~/assets/lightgallery/css/lightgallery.min.css" rel="stylesheet">
    <link href="~/assets/lightgallery/css/lg-transitions.min.css" rel="stylesheet">

    <!-- Alertas e Modais -->
    <link href="~/assets/css/sweetalert2.css" rel="stylesheet">

    <link href="~/assets/css/main.css" rel="stylesheet">
</head>
<body>
    <main>
        <!-- destaque -->

        <div class="container-full container-header-inner">



            <div class="container-fluid nav-iza">
                <div class="container">
                    <nav class="navbar navbar-default navbar-theme">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <div class="collapse navbar-collapse" id="menu">
                            <a href="javascript:" class="btn-close"><i class="material-icons md-24">close</i></a>
                            <ul class="nav navbar-nav">
                                <li class="@ViewData["ClassMenuHome"]"><a asp-area="" asp-controller="Home" asp-action="Index">Início<span class="sr-only">(current)</span></a></li>
                                <li class="@ViewData["ClassMenuClientes"]"><a asp-area="" asp-controller="Clientes" asp-action="Index">Clientes</a></li>
                                <li class="@ViewData["ClassMenuProdutos"]"><a asp-area="" asp-controller="Produtos" asp-action="Index">Produtos</a></li>
                                <li class="@ViewData["ClassMenuPedidos"]"><a asp-area="" asp-controller="Pedidos" asp-action="Index">Pedidos</a></li>
                                <li class="@ViewData["ClassMenuRelatorio"]"><a asp-area="" asp-controller="Relatorio" asp-action="Index">Relatório</a></li>
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>


            <div class="bgs-header">
                <div class="container title-page">
                    <div class="row">
                        <div class="col-md-12">

                            <h1><i class="material-icons">@ViewData["IconTitle"]</i> @ViewData["Title"]</h1>
                        </div>
                    </div>
                </div>
            </div>

        </div>

        <!-- fim do destaque -->

        <div class="bg-content inner">

            <div class="container-fluid" style="min-height: 700px">
                <div class="container content">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="content-page">
                                <form>
                                    @RenderBody()
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


                <!-- Rodapé -->
                <footer>
                    <div class="container-fluid footer-secondary">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-12">

                                    <p class="footer-address">
                                        <span class="text">© 2021 Izabely Furtado. Todos os direitos reservados.</span>
                                        <a href="tel:(27)99708-8212">
                                            <span class="footer-tel"><small>27</small> 9 9708-8212</span>
                                        </a>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>

                </footer>
            </div>
    </main>
    <!-- JS's -->
    <script src="~/assets/js/jquery.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/owl.carousel.min.js"></script>

    <!-- Light Gallery -->
    <script src="~/assets/lightgallery/js/jquery.mousewheel.min.js"></script>
    <script src="~/assets/lightgallery/js/lightgallery-all.min.js"></script>

    <!-- Mascaras Formulário -->
    <script src="~/assets/js/jquery.mask.min.js"></script>
    <script src="~/assets/js/mask.js"></script>

    <!-- Alertas e Modais -->
    <script src="~/assets/js/sweetalert2.min.js"></script>

    <script src="~/assets/js/main.js"></script>
</body>
</html>
